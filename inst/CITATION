citHeader("To cite 'primer.data' in publications use:")

citEntry(
  entry = "Manual",
  title = "primer.data",
  year = "2021",
  url  = "https://github.com/PPBDS/primer.data",
  textVersion = paste("Kane, D., & Weiss, T. (2021), 'primer.data'. R package version 0.7.0, <https://github.com/PPBDS/primer.data>."))
